CREATE TABLE PUBLISHERS (
PUBLISHER_ID INTEGER PRIMARY KEY AUTOINCREMENT,
PUBLISHER_NAME TEXT
);

allPublishers:
SELECT *
FROM PUBLISHERS;

getPublisherID:
SELECT PUBLISHER_ID
FROM PUBLISHERS
WHERE PUBLISHER_NAME = ?;

newPublisher:
INSERT INTO PUBLISHERS (PUBLISHER_NAME)
VALUES (?);

editPublisher:
UPDATE PUBLISHERS SET PUBLISHER_NAME = ?
WHERE PUBLISHER_ID = ?;